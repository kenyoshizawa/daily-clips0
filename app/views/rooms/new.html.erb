<h2>Room作成</h2>
<%= form_for(@room) do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :comment %>
  <%= f.text_field :comment %>

  <%= f.label :url%>
  <%= f.text_field :url %>

  <%= f.submit %>
<% end %>
<%= link_to '一覧に戻る', root_path %>
